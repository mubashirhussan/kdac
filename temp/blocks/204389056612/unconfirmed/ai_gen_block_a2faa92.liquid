{% doc %}
  @prompt
    Convert the brands grid section into a responsive slider/carousel that works on both desktop and mobile devices. The slider should allow users to navigate through brands using arrow buttons and should be touch-enabled for mobile swiping. Include smooth transitions and ensure the slider is fully responsive across all screen sizes.
{% enddoc %}
{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-brands-carousel-{{ ai_gen_id }} {
    width: 100%;
    padding: {{ block.settings.section_padding_top }}px 0 {{ block.settings.section_padding_bottom }}px;
    background-color: {{ block.settings.background_color }};
  }

  .ai-brands-carousel__container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 20px;
    position: relative;
  }

  .ai-brands-carousel__header-{{ ai_gen_id }} {
    text-align: {{ block.settings.heading_alignment }};
    margin-bottom: {{ block.settings.heading_spacing }}px;
  }

  .ai-brands-carousel__heading-{{ ai_gen_id }} {
    font-size: {{ block.settings.heading_size }}px;
    color: {{ block.settings.heading_color }};
    margin: 0;
  }

  .ai-brands-carousel__wrapper-{{ ai_gen_id }} {
    position: relative;
    overflow: hidden;
    margin: 0 {{ block.settings.arrow_spacing }}px;
  }

  .ai-brands-carousel__track-{{ ai_gen_id }} {
    display: flex;
    transition: transform 0.4s cubic-bezier(0.4, 0, 0.2, 1);
    gap: {{ block.settings.slide_gap }}px;
  }

  .ai-brands-carousel__slide-{{ ai_gen_id }} {
    flex: 0 0 calc((100% - ({{ block.settings.slides_desktop | minus: 1 }} * {{ block.settings.slide_gap }}px)) / {{ block.settings.slides_desktop }});
    display: flex;
    align-items: center;
    justify-content: center;
    padding: {{ block.settings.slide_padding }}px;
    background-color: {{ block.settings.slide_background }};
    border-radius: {{ block.settings.slide_border_radius }}px;
    border: {{ block.settings.slide_border_width }}px solid {{ block.settings.slide_border_color }};
    transition: all 0.3s ease;
    min-height: {{ block.settings.slide_min_height }}px;
  }

  .ai-brands-carousel__slide-{{ ai_gen_id }}:hover {
    transform: translateY(-{{ block.settings.hover_lift }}px);
    box-shadow: 0 {{ block.settings.hover_lift | times: 2 }}px {{ block.settings.hover_lift | times: 4 }}px rgba(0, 0, 0, 0.1);
  }

  .ai-brands-carousel__image-{{ ai_gen_id }} {
    max-width: 100%;
    max-height: {{ block.settings.image_max_height }}px;
    width: auto;
    height: auto;
    object-fit: contain;
    filter: grayscale({{ block.settings.image_grayscale }}%);
    opacity: {{ block.settings.image_opacity | divided_by: 100.0 }};
    transition: all 0.3s ease;
  }

  .ai-brands-carousel__slide-{{ ai_gen_id }}:hover .ai-brands-carousel__image-{{ ai_gen_id }} {
    filter: grayscale(0%);
    opacity: 1;
  }

  .ai-brands-carousel__arrow-{{ ai_gen_id }} {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    width: {{ block.settings.arrow_size }}px;
    height: {{ block.settings.arrow_size }}px;
    background-color: {{ block.settings.arrow_background }};
    border: none;
    border-radius: {{ block.settings.arrow_border_radius }}px;
    cursor: pointer;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 10;
    transition: all 0.3s ease;
    box-shadow: 0 2px 8px rgba(0, 0, 0, 0.15);
  }

  .ai-brands-carousel__arrow-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.arrow_hover_background }};
    transform: translateY(-50%) scale(1.1);
  }

  .ai-brands-carousel__arrow-{{ ai_gen_id }}:disabled {
    opacity: 0.3;
    cursor: not-allowed;
  }

  .ai-brands-carousel__arrow-{{ ai_gen_id }}:disabled:hover {
    transform: translateY(-50%) scale(1);
  }

  .ai-brands-carousel__arrow--prev-{{ ai_gen_id }} {
    left: 0;
  }

  .ai-brands-carousel__arrow--next-{{ ai_gen_id }} {
    right: 0;
  }

  .ai-brands-carousel__arrow-{{ ai_gen_id }} svg {
    width: {{ block.settings.arrow_icon_size }}px;
    height: {{ block.settings.arrow_icon_size }}px;
    stroke: {{ block.settings.arrow_color }};
    fill: none;
  }

  .ai-brands-carousel__dots-{{ ai_gen_id }} {
    display: flex;
    justify-content: center;
    gap: 8px;
    margin-top: {{ block.settings.dots_spacing }}px;
  }

  .ai-brands-carousel__dot-{{ ai_gen_id }} {
    width: {{ block.settings.dot_size }}px;
    height: {{ block.settings.dot_size }}px;
    border-radius: 50%;
    background-color: {{ block.settings.dot_color }};
    border: none;
    cursor: pointer;
    transition: all 0.3s ease;
    padding: 0;
  }

  .ai-brands-carousel__dot-{{ ai_gen_id }}.active {
    background-color: {{ block.settings.dot_active_color }};
    transform: scale(1.2);
  }

  @media screen and (max-width: 989px) {
    .ai-brands-carousel__slide-{{ ai_gen_id }} {
      flex: 0 0 calc((100% - ({{ block.settings.slides_tablet | minus: 1 }} * {{ block.settings.slide_gap }}px)) / {{ block.settings.slides_tablet }});
    }
  }

  @media screen and (max-width: 749px) {
    .ai-brands-carousel__slide-{{ ai_gen_id }} {
      flex: 0 0 calc((100% - ({{ block.settings.slides_mobile | minus: 1 }} * {{ block.settings.slide_gap }}px)) / {{ block.settings.slides_mobile }});
    }

    .ai-brands-carousel__heading-{{ ai_gen_id }} {
      font-size: {{ block.settings.heading_size | times: 0.7 }}px;
    }

    .ai-brands-carousel__arrow-{{ ai_gen_id }} {
      width: {{ block.settings.arrow_size | times: 0.8 }}px;
      height: {{ block.settings.arrow_size | times: 0.8 }}px;
    }

    .ai-brands-carousel__arrow-{{ ai_gen_id }} svg {
      width: {{ block.settings.arrow_icon_size | times: 0.8 }}px;
      height: {{ block.settings.arrow_icon_size | times: 0.8 }}px;
    }
  }
{% endstyle %}

<brands-carousel-{{ ai_gen_id }} class="ai-brands-carousel-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-brands-carousel__container-{{ ai_gen_id }}">
    {% if block.settings.heading != blank %}
      <div class="ai-brands-carousel__header-{{ ai_gen_id }}">
        <h2 class="ai-brands-carousel__heading-{{ ai_gen_id }}">{{ block.settings.heading }}</h2>
      </div>
    {% endif %}

    <div class="ai-brands-carousel__wrapper-{{ ai_gen_id }}">
      {% if block.settings.show_arrows %}
        <button
          class="ai-brands-carousel__arrow-{{ ai_gen_id }} ai-brands-carousel__arrow--prev-{{ ai_gen_id }}"
          aria-label="Previous slide"
          data-prev
        >
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <polyline points="15 18 9 12 15 6"></polyline>
          </svg>
        </button>
      {% endif %}

      <div class="ai-brands-carousel__track-{{ ai_gen_id }}" data-track>
        {% for i in (1..12) %}
          {% liquid
            assign image_key = 'brand_image_' | append: i
            assign brand_image = block.settings[image_key]
            assign url_key = 'brand_url_' | append: i
            assign brand_url = block.settings[url_key]
          %}

          {% if brand_image != blank %}
            <div class="ai-brands-carousel__slide-{{ ai_gen_id }}">
              {% if brand_url != blank %}
                <a href="{{ brand_url }}" target="_blank" rel="noopener">
                  <img
                    src="{{ brand_image | image_url: width: 400 }}"
                    alt="{{ brand_image.alt | escape }}"
                    class="ai-brands-carousel__image-{{ ai_gen_id }}"
                    loading="lazy"
                    width="400"
                    height="auto"
                  >
                </a>
              {% else %}
                <img
                  src="{{ brand_image | image_url: width: 400 }}"
                  alt="{{ brand_image.alt | escape }}"
                  class="ai-brands-carousel__image-{{ ai_gen_id }}"
                  loading="lazy"
                  width="400"
                  height="auto"
                >
              {% endif %}
            </div>
          {% endif %}
        {% endfor %}
      </div>

      {% if block.settings.show_arrows %}
        <button
          class="ai-brands-carousel__arrow-{{ ai_gen_id }} ai-brands-carousel__arrow--next-{{ ai_gen_id }}"
          aria-label="Next slide"
          data-next
        >
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <polyline points="9 18 15 12 9 6"></polyline>
          </svg>
        </button>
      {% endif %}
    </div>

    {% if block.settings.show_dots %}
      <div class="ai-brands-carousel__dots-{{ ai_gen_id }}" data-dots></div>
    {% endif %}
  </div>
</brands-carousel-{{ ai_gen_id }}>

<script>
  (function() {
    class BrandsCarousel{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.currentIndex = 0;
        this.slidesPerView = {{ block.settings.slides_desktop }};
        this.autoplayInterval = null;
        this.touchStartX = 0;
        this.touchEndX = 0;
        this.isDragging = false;
      }

      connectedCallback() {
        this.track = this.querySelector('[data-track]');
        this.slides = Array.from(this.track.children);
        this.prevButton = this.querySelector('[data-prev]');
        this.nextButton = this.querySelector('[data-next]');
        this.dotsContainer = this.querySelector('[data-dots]');

        if (this.slides.length === 0) return;

        this.updateSlidesPerView();
        this.setupEventListeners();
        this.createDots();
        this.updateCarousel();

        {% if block.settings.autoplay %}
          this.startAutoplay();
        {% endif %}

        window.addEventListener('resize', () => this.handleResize());
      }

      updateSlidesPerView() {
        const width = window.innerWidth;
        if (width <= 749) {
          this.slidesPerView = {{ block.settings.slides_mobile }};
        } else if (width <= 989) {
          this.slidesPerView = {{ block.settings.slides_tablet }};
        } else {
          this.slidesPerView = {{ block.settings.slides_desktop }};
        }
      }

      setupEventListeners() {
        if (this.prevButton) {
          this.prevButton.addEventListener('click', () => this.prev());
        }
        if (this.nextButton) {
          this.nextButton.addEventListener('click', () => this.next());
        }

        this.track.addEventListener('touchstart', (e) => this.handleTouchStart(e), { passive: true });
        this.track.addEventListener('touchmove', (e) => this.handleTouchMove(e), { passive: true });
        this.track.addEventListener('touchend', () => this.handleTouchEnd());

        this.track.addEventListener('mousedown', (e) => this.handleMouseDown(e));
        this.track.addEventListener('mousemove', (e) => this.handleMouseMove(e));
        this.track.addEventListener('mouseup', () => this.handleMouseUp());
        this.track.addEventListener('mouseleave', () => this.handleMouseUp());
      }

      handleTouchStart(e) {
        this.touchStartX = e.touches[0].clientX;
        this.stopAutoplay();
      }

      handleTouchMove(e) {
        this.touchEndX = e.touches[0].clientX;
      }

      handleTouchEnd() {
        const diff = this.touchStartX - this.touchEndX;
        const threshold = 50;

        if (Math.abs(diff) > threshold) {
          if (diff > 0) {
            this.next();
          } else {
            this.prev();
          }
        }

        {% if block.settings.autoplay %}
          this.startAutoplay();
        {% endif %}
      }

      handleMouseDown(e) {
        this.isDragging = true;
        this.touchStartX = e.clientX;
        this.track.style.cursor = 'grabbing';
        this.stopAutoplay();
      }

      handleMouseMove(e) {
        if (!this.isDragging) return;
        this.touchEndX = e.clientX;
      }

      handleMouseUp() {
        if (!this.isDragging) return;
        this.isDragging = false;
        this.track.style.cursor = 'grab';

        const diff = this.touchStartX - this.touchEndX;
        const threshold = 50;

        if (Math.abs(diff) > threshold) {
          if (diff > 0) {
            this.next();
          } else {
            this.prev();
          }
        }

        {% if block.settings.autoplay %}
          this.startAutoplay();
        {% endif %}
      }

      createDots() {
        if (!this.dotsContainer) return;

        const totalPages = Math.ceil(this.slides.length / this.slidesPerView);
        this.dotsContainer.innerHTML = '';

        for (let i = 0; i < totalPages; i++) {
          const dot = document.createElement('button');
          dot.className = 'ai-brands-carousel__dot-{{ ai_gen_id }}';
          dot.setAttribute('aria-label', `Go to slide ${i + 1}`);
          dot.addEventListener('click', () => {
            this.currentIndex = i * this.slidesPerView;
            this.updateCarousel();
            this.stopAutoplay();
            {% if block.settings.autoplay %}
              this.startAutoplay();
            {% endif %}
          });
          this.dotsContainer.appendChild(dot);
        }
      }

      updateCarousel() {
        const maxIndex = Math.max(0, this.slides.length - this.slidesPerView);
        this.currentIndex = Math.min(this.currentIndex, maxIndex);

        const slideWidth = this.slides[0].offsetWidth;
        const gap = {{ block.settings.slide_gap }};
        const offset = -(this.currentIndex * (slideWidth + gap));

        this.track.style.transform = `translateX(${offset}px)`;

        if (this.prevButton) {
          this.prevButton.disabled = this.currentIndex === 0;
        }
        if (this.nextButton) {
          this.nextButton.disabled = this.currentIndex >= maxIndex;
        }

        this.updateDots();
      }

      updateDots() {
        if (!this.dotsContainer) return;

        const dots = this.dotsContainer.querySelectorAll('.ai-brands-carousel__dot-{{ ai_gen_id }}');
        const activePage = Math.floor(this.currentIndex / this.slidesPerView);

        dots.forEach((dot, index) => {
          if (index === activePage) {
            dot.classList.add('active');
          } else {
            dot.classList.remove('active');
          }
        });
      }

      prev() {
        if (this.currentIndex > 0) {
          this.currentIndex = Math.max(0, this.currentIndex - this.slidesPerView);
          this.updateCarousel();
        }
      }

      next() {
        const maxIndex = Math.max(0, this.slides.length - this.slidesPerView);
        if (this.currentIndex < maxIndex) {
          this.currentIndex = Math.min(maxIndex, this.currentIndex + this.slidesPerView);
          this.updateCarousel();
        }
      }

      startAutoplay() {
        this.stopAutoplay();
        this.autoplayInterval = setInterval(() => {
          const maxIndex = Math.max(0, this.slides.length - this.slidesPerView);
          if (this.currentIndex >= maxIndex) {
            this.currentIndex = 0;
          } else {
            this.next();
          }
        }, {{ block.settings.autoplay_speed }} * 1000);
      }

      stopAutoplay() {
        if (this.autoplayInterval) {
          clearInterval(this.autoplayInterval);
          this.autoplayInterval = null;
        }
      }

      handleResize() {
        const oldSlidesPerView = this.slidesPerView;
        this.updateSlidesPerView();

        if (oldSlidesPerView !== this.slidesPerView) {
          this.createDots();
          this.updateCarousel();
        }
      }

      disconnectedCallback() {
        this.stopAutoplay();
        window.removeEventListener('resize', () => this.handleResize());
      }
    }

    customElements.define('brands-carousel-{{ ai_gen_id }}', BrandsCarousel{{ ai_gen_id }});
  })();
</script>

{% schema %}
{
  "name": "Brands carousel",
  "settings": [
    {
      "type": "header",
      "content": "Heading"
    },
    {
      "type": "text",
